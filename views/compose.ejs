<%- include("partials/header" ) -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="title">Title</label>
    <input class="form-control" type="text" name="composedTitle" id="title" />
  </div>
  <div class="form-group">
    <label for="body">Post</label>
    <textarea
      class="form-control"
      name="composedBody"
      id="body"
      cols="60"
      rows="10"
    ></textarea>
  </div>

  <button class="btn btn-md btn-primary" type="submit" name="button">
    Publish
  </button>
</form>
<%- include("partials/footer" ) -%>
